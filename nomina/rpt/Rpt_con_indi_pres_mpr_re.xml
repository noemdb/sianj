<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE REPORT SYSTEM "PHPReport.dtd"> 
<REPORT xmlns:xi="http://www.w3.org/2001/XInclude">
	<TITLE>CONTROL INDIVIDUAL DE PRESTACIONES E INTERESES</TITLE>
	<BACKGROUND_COLOR>#FFFFFF</BACKGROUND_COLOR>
	<CSS>sia.css</CSS>
   <DOCUMENT>   
       <FOOTER BORDER="0" CELLSPACING="0" CELLPADDING="3" WIDTH="500">
		 <ROW>
         </ROW>
      </FOOTER>   
   </DOCUMENT>
	<PAGE BORDER="0" SIZE="35" CELLSPACING="0" CELLPADDING="3" WIDTH="940"> 
	<HEADER>
      <ROW>
		<COL COLSPAN="17" CELLCLASS="PAGE_LAYER" TEXTCLASS="MEDIABOLD">
			<XHTML>
				<TABLE BORDER="0" CELLPADDING="2" CELLSPACING="0" WIDTH="100%">
					<TR>
					    <TD width="930" rowspan="4">
						  <IMG SRC="../../imagenes/Encabezado_rpt_nomina.png" WIDTH="930" HEIGHT="49" BORDER="0"/>						  
						</TD>
				    </TR>		
				</TABLE>				
			</XHTML>
		</COL>
	 </ROW>
	<ROW>
	   <COL ALIGN="CENTER" COLSPAN="17" HEIGHT="40" TEXTCLASS="MEDIABOLD"></COL>
	</ROW>			   
	<ROW>
		<COL COLSPAN="17" CELLCLASS="PAGE_LAYER" TEXTCLASS="MEDIABOLD">
			<XHTML>
				<TABLE BORDER="1" CELLPADDING="2" CELLSPACING="0" WIDTH="100%">
					<TR>
						<TD ALIGN="CENTER" TEXTCLASS="MEDIABOLD"><font color="Navy"><b>CONTROL INDIVIDUAL DE PRESTACIONES E INTERESES</b></font></TD>
					</TR>
				   </TABLE>				
			</XHTML>
		</COL>	
	  </ROW>		      
   </HEADER>
   </PAGE>
   	<GROUPS>
		<GROUP REPRINT_HEADER_ON_PAGEBREAK="TRUE" NAME="cod_empleados" EXPRESSION="cod_empleado">
		    <HEADER>
				<ROW>
	   				<COL ALIGN="CENTER"  COLSPAN="1" TEXTCLASS="MEDIABOLD">Empleado</COL>
	   				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("cod_empleado")</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="15">$this->getValue("nombre")</COL>  
				</ROW>
				<ROW>
	   				<COL ALIGN="CENTER" COLSPAN="1" TEXTCLASS="MEDIABOLD">Cedula</COL>
	   				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="16">$this->getValue("cedula")</COL> 
				</ROW>
				<ROW>
	   				<COL ALIGN="CENTER"  COLSPAN="1" TEXTCLASS="MEDIABOLD">Fecha Ingreso</COL>
	   				<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("fechai")</COL>
					<COL ALIGN="CENTER"  COLSPAN="1" TEXTCLASS="MEDIABOLD">Fecha Corte</COL>
					<COL TYPE="EXPRESSION" ALIGN="LEFT" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="14"></COL>  
				</ROW>
				<ROW>
         			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Fecha Calculo</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Sueldo</COL>
		 			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Dias Prestaciones</COL>
         			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Monto Prestaciones</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Total Prestaciones</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Monto Adelanto</COL>
		 			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Total Adelanto</COL>
         			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Saldo Prestaciones</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Tiempo</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Interes Aplica</COL>
		 			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Interes Devengado</COL>
         			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Interes Pagado</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Interes No Capitalizado</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Interes Capitalizados</COL>
		 			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Total Interes</COL>
         			<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Acumulado Total</COL>
					<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Tipo Mov.</COL>
     		 </ROW>	
			 </HEADER>
			<FOOTER> 
			</FOOTER>
		<GROUP REPRINT_HEADER_ON_PAGEBREAK="TRUE" NAME="detalles" EXPRESSION="detalle">
			 <HEADER> </HEADER>
			 <FOOTER> </FOOTER>
             <FIELDS>
   			  <ROW>
			  	<COL TEXTCLASS="MEDIA" ALIGN="LEFT" TYPE="FIELD">fechac</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" TYPE="FIELD">sueldo_calculo</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" TYPE="FIELD">dias_prestaciones</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">c_prestaciones</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" NUMBERFORMATEX="2" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("total_prestaciones")-$this->getValue("c_presta_adic")</COL> 
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">monto_adelanto</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">total_adelanto</COL>
				<COL TYPE="EXPRESSION" ALIGN="CENTER" NUMBERFORMATEX="2" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("saldo_prestaciones")-$this->getValue("c_presta_adic")</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" TYPE="FIELD">tiempo_variacion</COL> 
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">tasa_interes</COL> 
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">interes_devengado</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">interes_pagado</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">interes_noacum</COL>  
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">interes_acum</COL> 
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">total_interes</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" NUMBERFORMATEX="2" TYPE="FIELD">acumulado_total</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" TYPE="FIELD">tipo_calculo</COL>
			  </ROW>
			</FIELDS>   
		</GROUP>
	   </GROUP>
	</GROUPS>
</REPORT>
