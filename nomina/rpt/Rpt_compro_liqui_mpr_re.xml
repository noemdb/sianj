	<GROUPS>
		<GROUP REPRINT_HEADER_ON_PAGEBREAK="TRUE" NAME="cod_empleado" EXPRESSION="cod_empleado">
		   <HEADER>
   			 <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Trabajador:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getValue("cod_empleado")."      ".$this->getValue("nombre")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Cedula:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getValue("cedula")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Cargo:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getValue("des_cargo")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Fecha Ingreso:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="3">$this->getValue("fecha_ingreso")</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Fecha Liquidacion:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="3">$this->getValue("fecha_liquidacion")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Tiempo Trabajado:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("ant_ano")</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Años:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("ant_mes")</COL> 
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Meses:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="1">$this->getValue("ant_dia")</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="2" TYPE="REGULAR">Dias:</COL>
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Ultimo Sueldo:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getValue("sueldo_basico")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Sueldo Diario:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getValue("sueldo_basico")/30</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Fecha:</COL>
			 	 <COL TYPE="EXPRESSION" CELLCLASS="DOCUMENT_LAYER" TEXTCLASS="MEDIA" COLSPAN="7">$this->getParameter("date")</COL> 
			   </ROW>
			   <ROW>
			     <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="2" TYPE="REGULAR">Descripcion Concepto</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Monto</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">Cant.</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="2" TYPE="REGULAR">Asignaciones</COL>
				 <COL ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="2" TYPE="REGULAR">Deducciones</COL>
			   </ROW>
           </HEADER>
		   <FOOTER>
             <ROW>
			 	<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="DOCUMENT_LAYER" NUMBERFORMATEX="2" TEXTCLASS="MEDIABOLD" COLSPAN="4"></COL>
			 	<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="DOCUMENT_LAYER" NUMBERFORMATEX="2" TEXTCLASS="MEDIABOLD" COLSPAN="2">$this->getSum("monto_asignacion")</COL> 
				<COL TYPE="EXPRESSION" ALIGN="CENTER" CELLCLASS="DOCUMENT_LAYER" NUMBERFORMATEX="2" TEXTCLASS="MEDIABOLD" COLSPAN="2">$this->getSum("monto_deduccion")</COL> 
			 </ROW>
			 <ROW>
			 	<COL CELLCLASS="GROUP_LAYER" ALIGN="CENTER" TEXTCLASS="MEDIABOLD" COLSPAN="1" TYPE="REGULAR">NETO A PAGAR</COL>
			</ROW>
           </FOOTER>
		<GROUP REPRINT_HEADER_ON_PAGEBREAK="TRUE" NAME="Detalles" EXPRESSION="detalles">
			 <HEADER> 
			 </HEADER>
			 <FOOTER> 
			 </FOOTER>
			 <FIELDS>
   			  <ROW>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" COLSPAN="2" TYPE="FIELD">den_concepto</COL>	
                <COL TEXTCLASS="MEDIA" ALIGN="LEFT" COLSPAN="1" TYPE="FIELD">monto_base</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" COLSPAN="2" TYPE="FIELD">cantidad</COL>
                <COL TEXTCLASS="MEDIA" ALIGN="LEFT" COLSPAN="2" TYPE="FIELD">monto_asignacion</COL>
				<COL TEXTCLASS="MEDIA" ALIGN="LEFT" COLSPAN="2" TYPE="FIELD">monto_deduccion</COL>
			  </ROW>
			</FIELDS>   
		</GROUP>
	   </GROUP>
	</GROUPS>
</REPORT>
