				<COL ALIGN="CENTER" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">status</COL>
				<COL ALIGN="CENTER" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">fecha_ingreso</COL>
			  </ROW>
			  <ROW>        		
         		<COL  ALIGN="LEFT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Tipo</COL>
