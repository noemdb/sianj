            </ROW>
           </HEADER>
		   <FOOTER>
           </FOOTER>
			<GROUP REPRINT_HEADER_ON_PAGEBREAK="TRUE" NAME="detalle" EXPRESSION="detalles">
			 <HEADER> 
			 </HEADER>
			 <FOOTER> </FOOTER>
             <FIELDS>
   			  <ROW>
				<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">cod_empleado</COL>	
