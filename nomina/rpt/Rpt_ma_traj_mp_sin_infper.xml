		  		<COL  ALIGN="CENTER" TEXTCLASS="MEDIABOLD" TYPE="REGULAR" COLSPAN="2">Fecha Asignacion</COL>
		  		<COL  ALIGN="CENTER" TEXTCLASS="MEDIABOLD" TYPE="REGULAR" COLSPAN="2">Sueldo Cargo</COL>  
