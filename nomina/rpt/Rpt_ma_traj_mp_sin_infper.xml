			  <ROW>
				<COL ALIGN="LEFT" COLSPAN="2" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Cod. Departamento:</COL> 
			  	<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">cod_departamento</COL>
				<COL ALIGN="RIGHT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Departamento:</COL> 
			  	<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="4" TYPE="FIELD">des_departamento</COL>
			  </ROW>
			  <ROW>
				<COL  ALIGN="CENTER" TEXTCLASS="MEDIABOLD" TYPE="REGULAR" COLSPAN="8"></COL>
			  </ROW> 
			 
			</FIELDS>   
