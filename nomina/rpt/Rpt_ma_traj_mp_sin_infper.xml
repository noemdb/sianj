			  </ROW>
			  <ROW>
			  	<COL  ALIGN="LEFT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Nacionalidad:</COL>
				<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">nacionalidad</COL>	
		  		<COL  ALIGN="LEFT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Codigo:</COL>
				<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">cod_banco</COL>	
		  		<COL  ALIGN="LEFT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Cuenta Empresa:</COL>
				<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">cta_empresa</COL>	      
				<COL  ALIGN="LEFT" COLSPAN="1" TEXTCLASS="MEDIABOLD" TYPE="REGULAR">Nombre del Banco:</COL>      
				<COL ALIGN="LEFT" TEXTCLASS="MEDIA" COLSPAN="1" TYPE="FIELD">nombre_banco</COL>
			  </ROW>
			  <ROW>        		
         		<COL  ALIGN="CENTER" TEXTCLASS="MEDIABOLD" TYPE="REGULAR" COLSPAN="8">INFORMACION DE ASIGNACION DE CARGOS</COL>       
            </ROW> 
			<ROW>
				<COL  ALIGN="LEFT" TEXTCLASS="MEDIABOLD" TYPE="REGULAR" COLSPAN="1">Cod. Cargo</COL>
